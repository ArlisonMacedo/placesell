{
  "synchronize": true,
    "logging": false,
    "entities": [
        "dist/entity/*.ts"
    ],
    "migrations": [
        "dist/migration/*.ts"
    ],
    "subscribers": [
        "src/subscriber/*.ts"
    ],
    "cli": {
        "entitiesDir": "src/entity",
        "migrationsDir": "src/migration",
        "subscribersDir": "src/subscriber"
    },
    "type": "sqlite",
    "database": "dist/database/database.sqlite"
}